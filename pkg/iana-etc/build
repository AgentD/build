}

deploy() {
	local SOURCE="$1"
	local BUILD="$2"
	local DEPLOY="$3"

	mkdir -p "$DEPLOY/etc"
	cp "$SOURCE/protocols" "$DEPLOY/etc"
	cp "$SOURCE/services" "$DEPLOY/etc"
}
