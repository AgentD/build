	return
}

deploy() {
	touch "$PKGDEPLOYDIR/$PKGNAME.files"
	touch "$PKGDEPLOYDIR/$PKGNAME.desc"
}

check_update() {
	check_update_simple "$URL" "mpfr-" "tar.xz"
}